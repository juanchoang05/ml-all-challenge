{
  "name": "ml_item_product",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "dev:local": "cross-env REACT_APP_USE_MOCK_DATA=true REACT_APP_REGION=colombia vite",
    "dev:testing": "cross-env REACT_APP_ENV=testing REACT_APP_REGION=colombia vite",
    "dev:production": "cross-env REACT_APP_ENV=production REACT_APP_REGION=colombia vite build && vite preview",
    "dev:colombia": "cross-env REACT_APP_ENV=development REACT_APP_REGION=colombia vite",
    "dev:argentina": "cross-env REACT_APP_ENV=development REACT_APP_REGION=argentina vite",
    "dev:mexico": "cross-env REACT_APP_ENV=development REACT_APP_REGION=mexico vite",
    "dev:brazil": "cross-env REACT_APP_ENV=development REACT_APP_REGION=brazil vite",
    "dev:chile": "cross-env REACT_APP_ENV=development REACT_APP_REGION=chile vite",
    "test:colombia": "cross-env REACT_APP_ENV=testing REACT_APP_REGION=colombia vite",
    "test:argentina": "cross-env REACT_APP_ENV=testing REACT_APP_REGION=argentina vite",
    "test:mexico": "cross-env REACT_APP_ENV=testing REACT_APP_REGION=mexico vite",
    "build:production": "cross-env REACT_APP_ENV=production REACT_APP_REGION=colombia vite build",
    "build:colombia": "cross-env REACT_APP_ENV=production REACT_APP_REGION=colombia vite build",
    "build:argentina": "cross-env REACT_APP_ENV=production REACT_APP_REGION=argentina vite build",
    "build:mexico": "cross-env REACT_APP_ENV=production REACT_APP_REGION=mexico vite build",
    "env:switch": "node scripts/env-switch.js",
    "env:status": "echo 'Environment Status:' && test -f .env && cat .env || echo 'No .env file found'",
    "env:help": "node scripts/env-switch.js --help",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "test:mocks": "node src/test-mocks.js",
    "verify:mocks": "node src/test-mocks.js --help",
    "test:ui-integration": "node src/test-ui-integration-simple.js"
  },
  "dependencies": {
    "react": "^19.1.0",
    "react-dom": "^19.1.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.30.1",
    "@testing-library/jest-dom": "^6.6.3",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@vitejs/plugin-react": "^4.6.0",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "cross-env": "^7.0.3",
    "eslint": "^9.30.1",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.3.0",
    "jsdom": "^26.1.0",
    "vite": "^7.0.4",
    "vitest": "^3.2.4"
  }
}
